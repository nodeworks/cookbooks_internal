client
dev tun 
proto udp
port 1194
resolv-retry infinite
nobind
<% @remote.each do |server| -%>
remote <%= server %>
<% end -%>
keepalive 10 60
inactive 600
persist-tun
persist-key
ping-timer-rem
#secret /etc/openvpn/static.key
dh /etc/openvpn/keys/dh1024.pem
ca /etc/openvpn/keys/ca.crt
cert /etc/openvpn/keys/client1.crt
key /etc/openvpn/keys/client1.key
log <%= @openvpn_log %>
verb 3
comp-lzo yes
status <%= @status_log %>
