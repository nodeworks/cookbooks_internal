<game>
        <minPlayers>2</minPlayers>
        <maxPlayers>2</maxPlayers>
        <maxResponses>10</maxResponses>
        <extensionName></extensionName>
        <extensionClass></extensionClass>
        <gameId>3013</gameId>
        <usersCanJoinActiveGames>false</usersCanJoinActiveGames>
        <serpentServer>http://<%= @serpent_endpoint %>:8080/v2/json.php</serpentServer>

        <database>
                <gametype>20</gametype>
                <url>jdbc:mysql://<%= @bullfrog_poker_db_host %>:3306/lobby?cacheCallableStmts=true&amp;autoReconnect=true&amp;interactiveClient=true</url>
                <username><%= @bullfrog_poker_username %></username>
                <password><%= @bullfrog_poker_password %></password>
        </database>

        <myServer>
                <id>20</id>
                <smartfoxHostname><%= @hostname %></smartfoxHostname>
                <smartfoxPort>9311</smartfoxPort>
                <interserverHostname><%= @hostname %></interserverHostname>
                <interserverport>10010</interserverport>
        </myServer>

</game>
